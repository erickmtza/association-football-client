{
    "version": 2,
    "name": "association-football",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "^/manifest.json$",
          "dest": "/manifest.json",
          "headers": {"Content-Type": "application/json"}
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ]
  }